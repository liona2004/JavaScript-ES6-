/*
Метод Object.freeze() замораживает объект: это значит, что он предотвращает добавление новых свойств к объекту, удаление старых свойств из объекта 
и изменение существующих свойств или значения их атрибутов

Выражения стрелочных функций имеют более короткий синтаксис по сравнению с функциональными выражениями (inline functions)
*/

Object.freeze(obj)


const myFunc = () => {
  const myVar = "value";
  return myVar;
}
// ИЛИ This code will still return the string value by default.
const myFunc = () => "value";

const myConcat = (arr1, arr2) => {
  return arr1.concat(arr2);
};

console.log(myConcat([1, 2], [3, 4, 5]));
const greeting = (name = "Anonymous") => "Hello " + name;
console.log(greeting("John"));
console.log(greeting());


/*
Метод Math.max() возвращает наибольшее из нуля или более чисел.
*/
const arr = [6, 89, 3, 45];
const maximus = Math.max(...arr);
maximus would have a value of 89.
